{
    "name": "sushiware.net",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "postbuild:sitemap": "next-sitemap",
        "postbuild:rss": "cross-env TS_NODE_PROJECT=\"./script-tsconfig.json\" ts-node generate-rss.ts --esModuleInterop",
        "postbuild:cname": "cpx -C CNAME out/CNAME",
        "postbuild:all": "npm run postbuild:sitemap && npm run postbuild:rss && npm run postbuild:cname",
        "build": "next build",
        "export": "next export",
        "deploy": "npm run build && npm run export && npm run postbuild:all",
        "dev": "next dev",
        "fix:eslint": "eslint . --ext .ts,.tsx --fix",
        "fix:prettier": "prettier --write . \"*.{ts, tsx}\"",
        "fix:all": "npm run fix:eslint && npm run fix:prettier",
        "ci": "npm run fix:all && npm run deploy"
    },
    "dependencies": {
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "classnames": "^2.2.6",
        "cross-env": "^7.0.3",
        "date-fns": "^2.16.1",
        "date-fns-tz": "^1.0.12",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "gray-matter": "^4.0.2",
        "highlight.js": "^11.1.0",
        "next": "^11.0.1",
        "next-sitemap": "^1.6.140",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-hot-loader": "^4.13.0",
        "remark": "^13.0.0",
        "remark-highlight.js": "^6.0.0",
        "remark-html": "^13.0.1",
        "sharp": "^0.29.0"
    },
    "devDependencies": {
        "@types/node": "^16.4.1",
        "@types/react": "^17.0.14",
        "cpx": "^1.5.0",
        "eslint-config-next": "^11.0.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.1.0",
        "typescript": "^4.0.5"
    }
}
